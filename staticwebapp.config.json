{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serveMatchingFiles": [
        "index.html"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*",
      "/css/*",
      "/api/*"
    ]
  }
}
